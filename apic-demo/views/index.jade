extends layout

block body
  div.container(ng-controller='AppCtrl')
    h2(ng-bind="name")
    ul.nav.nav-tabs.tabbg.nav-justified
      li(data-toggle="tab")
        a(href='movie-search') Movie Search
      li(data-toggle="tab")
        a(href='starExample') Star Example

    div.tab-content(ng-view)
    

  script(src='/lib/angular/angular.js')
  script(src='/lib/angular-route/angular-route.js')
  script(src='js/app.js')
  script(src='js/services.js')
  script(src='js/controllers.js')
  script(src='js/filters.js')
  script(src='js/directives.js')
  script(src='js/forcechart.js')